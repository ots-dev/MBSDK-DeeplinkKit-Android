<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.daimler.mbdeeplinkkit.sample">

    <application
        android:name="com.daimler.mbdeeplinkkit.sample.app.DeeplinkApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.daimler.mbdeeplinkkit.sample.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.daimler.mbdeeplinkkit.sample.overview.apps.FamilyAppsActivity" />
        <activity android:name="com.daimler.mbdeeplinkkit.sample.overview.deeplinks.DeepLinkOverviewActivity" />
    </application>

</manifest>